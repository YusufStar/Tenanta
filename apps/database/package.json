{
  "name": "@tenanta/database",
  "version": "1.0.0",
  "description": "Database schemas, migrations, and Redis configurations for Tenanta",
  "main": "src/index.ts",
  "scripts": {
    "migrate": "ts-node src/migrations/run-migrations.ts",
    "seed": "ts-node src/seeds/run-seeds.ts",
    "setup": "npm run migrate && npm run seed",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "dev": "ts-node src/index.ts"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "@tenanta/logging": "*"
  },
  "devDependencies": {
    "@types/pg": "^8.10.7",
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.5",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3"
  },
  "keywords": [
    "database",
    "postgresql",
    "redis",
    "migrations",
    "tenanta"
  ],
  "author": "Yusuf Yıldız",
  "license": "MIT"
} 